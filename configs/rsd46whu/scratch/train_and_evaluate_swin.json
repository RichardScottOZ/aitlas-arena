{
    "model": {
        "classname": "aitlas.models.SwinTransformer",
        "config": {
            "num_classes": 46,
            "learning_rate": 0.0001,
            "pretrained": false,
            "threshold": 0.5,
            "metrics": ["accuracy", "precision", "recall", "f1_score"]
        }
    },
    "task": {
        "classname": "aitlas.tasks.TrainAndEvaluateTask",
        "config": {
            "epochs": 100,
            "model_directory": "./data/experiments/rsd46whu/scratch",
            "save_epochs": 50,
            "id": "swin",
            "train_dataset_config": {
                "classname": "aitlas.datasets.RSD46WHUDataset",
                "config": {
                    "batch_size": 128,
                    "shuffle": true,
                    "num_workers": 4,
                    "data_dir": "./data/RSD46-WHU/train",
                    "csv_file": "./data/RSD46-WHU/train/train.csv",
					"transforms": ["aitlas.transforms.ResizeRandomCropFlipHVToTensor"]
                }
            },
			"val_dataset_config": {
                "classname": "aitlas.datasets.RSD46WHUDataset",
                "config": {
                    "batch_size": 128,
                    "shuffle": false,
                    "num_workers": 4,
                    "data_dir": "./data/RSD46-WHU/train",
                    "csv_file": "./data/RSD46-WHU/train/val.csv",
                    "transforms": ["aitlas.transforms.ResizeCenterCropToTensor"]
                }
            }
        }
    }
}
