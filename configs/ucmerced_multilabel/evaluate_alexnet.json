{
    "model": {
        "classname": "aitlas.models.AlexNetMultiLabel",
        "config": {
            "num_classes": 17,
            "threshold": 0.5,		         
            "metrics": ["precision", "recall", "f1_score", "map"]
        }
    },
    "task": {
        "classname": "aitlas.tasks.EvaluateTask",
        "config": {
            "model_path": "./data/experiments/ucmerced_multilabel/alexnet/checkpoint.pth.tar",
            "dataset_config": {
                "classname": "aitlas.datasets.UcMercedMultiLabelDataset",
                "config": {
                    "batch_size": 128,
                    "shuffle": false,
                    "num_workers": 4,
                    "data_dir": "./data/UCMerced_multilabel/images",
                    "csv_file": "./data/UCMerced_multilabel/test.csv",
                    "transforms": ["aitlas.transforms.ResizeCenterCropToTensor"]
                }
            }
        }
    }
}
