{
    "model": {
        "classname": "aitlas.models.VGG16",
        "config": {
            "num_classes": 25,            
            "metrics": ["accuracy", "precision", "recall", "f1_score"]
        }
    },
    "task": {
        "classname": "aitlas.tasks.EvaluateTask",
        "config": {
            "model_path": "./data/experiments/clrs/vgg16/checkpoint.pth.tar",
            "dataset_config": {
                "classname": "aitlas.datasets.CLRSDataset",
                "config": {
                    "batch_size": 128,
                    "shuffle": false,
                    "num_workers": 4,
                    "data_dir": "./data/multi-class/CLRS",
                    "csv_file": "./data/multi-class/CLRS/test.csv",
                    "transforms": ["aitlas.transforms.ResizeCenterCropToTensor"]
                }
            }
        }
    }
}
